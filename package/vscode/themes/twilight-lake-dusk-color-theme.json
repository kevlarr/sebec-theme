    "semanticTokenColors": {
        "class": "#77abc5",
        "enum": "#77abc5",
        "function": "#77abc5",
        "function.decorator": "#77abc5",
        "method": "#77abc5",
        "class.declaration": {
            "foreground": "#77abc5",
            "fontStyle": "bold"
        },
        "function.declaration": {
            "foreground": "#77abc5",
            "fontStyle": "bold"
        },
        "method.declaration": {
            "foreground": "#77abc5",
            "fontStyle": "bold"
        },
        "module": {
            "foreground": "#4685a4",
            "fontStyle": "underline"
        },
        "selfParameter": {
            "foreground": "#9f6f97",
            "fontStyle": "bold"
        },
        "clsParameter": {
            "foreground": "#9f6f97",
            "fontStyle": "bold"
        },
        "property": "#9f6f97",
        "property.declaration": "#9f6f97",
        "enumMember": "#9f6f97",
        "class.typeHint": "#4685a4",
        "enum.typeHint": "#4685a4"
    },
    "tokenColors": [
        {
            "scope": [
                "meta.function-call.generic.python",
                "support.function.string.sql"
            ],
            "settings": {
                "foreground": "#77abc5"
            }
        },
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#5e6a82",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "storage.type",
                "storage.type.class",
                "storage.type.function",
                "storage.modifier",
                "keyword.control.flow",
                "keyword.control.import",
                "keyword.operator.logical"
            ],
            "settings": {
                "foreground": "#465ea4",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "variable.parameter.function-call"
            ],
            "settings": {
                "foreground": "#465ea4"
            }
        },
        {
            "scope": [
                "meta.function-call.arguments keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#5e6a82"
            }
        },
        {
            "scope": [
                "constant.language",
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#91ae8f"
            }
        },
        {
            "scope": [
                "keyword.operator.arithmetic",
                "keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#465ea4"
            }
        },
        {
            "scope": [
                "punctuation.parenthesis",
                "punctuation.definition.arguments",
                "punctuation.separator",
                "punctuation.section.class.begin",
                "punctuation.section.function.begin"
            ],
            "settings": {
                "foreground": "#5e6a82"
            }
        },
        {
            "scope": [
                "punctuation.definition.decorator"
            ],
            "settings": {
                "foreground": "#295f7a"
            }
        },
        {
            "scope": [
                "keyword.other.DML.sql",
                "keyword.other.DDL",
                "keyword.other.alias.sql",
                "keyword.other.sql"
            ],
            "settings": {
                "foreground": "#465ea4"
            }
        },
        {
            "scope": [
                "constant.other.table-name.sql"
            ],
            "settings": {
                "foreground": "#9f6f97"
            }
        },
        {
            "scope": [
                "storage.type.string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#41653e"
            }
        },
        {
            "scope": [
                "constant.character.format.placeholder"
            ],
            "settings": {
                "foreground": "#41653e"
            }
        },
        {
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#668863",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#4685a4"
            }
        },
        {
            "scope": [
                "support.variable.magic",
                "constant.other.caps"
            ],
            "settings": {
                "foreground": "#bf6169"
            }
        },
        {
            "scope": [
                "support.type.property-name.json",
                "entity.name.tag.yaml"
            ],
            "settings": {
                "foreground": "#778bc5"
            }
        }
