    "semanticTokenColors": {
        "class": "#5eb8c9",
        "enum": "#5eb8c9",
        "function": "#5eb8c9",
        "function.decorator": "#5eb8c9",
        "method": "#5eb8c9",
        "class.declaration": {
            "foreground": "#5eb8c9",
            "fontStyle": "bold"
        },
        "function.declaration": {
            "foreground": "#5eb8c9",
            "fontStyle": "bold"
        },
        "method.declaration": {
            "foreground": "#5eb8c9",
            "fontStyle": "bold"
        },
        "module": {
            "foreground": "#3896a8",
            "fontStyle": "underline"
        },
        "selfParameter": {
            "foreground": "#9f6f97",
            "fontStyle": "bold"
        },
        "clsParameter": {
            "foreground": "#9f6f97",
            "fontStyle": "bold"
        },
        "property": "#9f6f97",
        "property.declaration": "#9f6f97",
        "enumMember": "#9f6f97",
        "class.typeHint": "#3896a8",
        "enum.typeHint": "#3896a8"
    },
    "tokenColors": [
        {
            "scope": [
                "meta.function-call.generic.python",
                "support.function.string.sql"
            ],
            "settings": {
                "foreground": "#5eb8c9"
            }
        },
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#5e6a82",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "storage.type.class",
                "storage.type.function",
                "storage.modifier",
                "keyword.control.flow",
                "keyword.control.import",
                "keyword.operator.logical"
            ],
            "settings": {
                "foreground": "#387aa8",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "variable.parameter.function-call"
            ],
            "settings": {
                "foreground": "#387aa8"
            }
        },
        {
            "scope": [
                "meta.function-call.arguments keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#5e6a82"
            }
        },
        {
            "scope": [
                "constant.language",
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#79b474"
            }
        },
        {
            "scope": [
                "keyword.operator.arithmetic",
                "keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#387aa8"
            }
        },
        {
            "scope": [
                "punctuation.parenthesis",
                "punctuation.definition.arguments",
                "punctuation.separator",
                "punctuation.section.class.begin",
                "punctuation.section.function.begin"
            ],
            "settings": {
                "foreground": "#5e6a82"
            }
        },
        {
            "scope": [
                "punctuation.definition.decorator"
            ],
            "settings": {
                "foreground": "#266673"
            }
        },
        {
            "scope": [
                "keyword.other.DML.sql",
                "keyword.other.DDL",
                "keyword.other.alias.sql",
                "keyword.other.sql"
            ],
            "settings": {
                "foreground": "#387aa8"
            }
        },
        {
            "scope": [
                "constant.other.table-name.sql"
            ],
            "settings": {
                "foreground": "#9f6f97"
            }
        },
        {
            "scope": [
                "storage.type.string",
                "punctuation.definition.string,"
            ],
            "settings": {
                "foreground": "#396336",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "constant.character.format.placeholder"
            ],
            "settings": {
                "foreground": "#396336"
            }
        },
        {
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#54924f",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "support.variable.magic",
                "constant.other.caps"
            ],
            "settings": {
                "foreground": "#bf6169"
            }
        },
        {
            "scope": [
                "support.type.property-name.json",
                "entity.name.tag.yaml"
            ],
            "settings": {
                "foreground": "#5e9dc9"
            }
        }
    ]
