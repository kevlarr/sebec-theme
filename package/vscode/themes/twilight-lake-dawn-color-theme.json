            "settings": {
                "foreground": "#8d95a5",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "keyword.operator.arithmetic",
                "keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#8d95a5"
            }
