        {
            "scope": [
                "punctuation.definition.decorator"
            ],
            "settings": {
                "foreground": "#9ba9d4"
            }
        },
        {
            "scope": [
                "keyword.other.DML.sql",
                "keyword.other.DDL",
                "keyword.other.alias.sql",
                "keyword.other.sql"
            ],
            "settings": {
                "foreground": "#77abc5"
            }
        },
        {
            "scope": [
                "constant.other.table-name.sql"
            ],
            "settings": {
                "foreground": "#653e5f"
            }
        },
        {
            "scope": [
                "storage.type.string",
