            "settings": {
                "foreground": "#668863"
            }
        },
        {
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#41653e",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#778bc5"
            }
        },
        {
            "scope": [
                "support.variable.magic",
                "constant.other.caps"
