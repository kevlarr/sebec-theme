    "semanticTokenColors": {
        "class": "#465ea4",
        "enum": "#465ea4",
        "function": "#465ea4",
        "function.decorator": "#465ea4",
        "method": "#465ea4",
        "class.declaration": {
            "foreground": "#465ea4",
            "fontStyle": "bold"
        },
        "function.declaration": {
            "foreground": "#465ea4",
            "fontStyle": "bold"
        },
        "method.declaration": {
            "foreground": "#465ea4",
            "fontStyle": "bold"
        },
        "module": {
            "foreground": "#4d576a",
            "fontStyle": "underline"
        },
        "selfParameter": {
            "foreground": "#b48eae",
            "fontStyle": "bold"
        },
        "clsParameter": {
            "foreground": "#b48eae",
            "fontStyle": "bold"
        },
        "property": "#84587c",
        "property.declaration": "#84587c",
        "enumMember": "#84587c",
        "class.typeHint": "#778bc5",
        "enum.typeHint": "#778bc5"
    },
    "tokenColors": [
        {
            "scope": [
                "meta.function-call.generic.python",
                "support.function.string.sql"
            ],
            "settings": {
                "foreground": "#465ea4"
            }
        },
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#9ea5b3",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "storage.type",
                "storage.type.class",
                "storage.type.function",
                "storage.modifier",
                "keyword.control.flow",
                "keyword.control.import",
                "keyword.operator.logical"
            ],
            "settings": {
                "foreground": "#4685a4"
            }
        },
        {
            "scope": [
                "variable.parameter.function-call"
            ],
            "settings": {
                "foreground": "#77abc5"
            }
        },
        {
            "scope": [
                "meta.function-call.arguments keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#9ea5b3"
            }
        },
        {
            "scope": [
                "constant.language",
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#6a8e67"
            }
        },
        {
            "scope": [
                "keyword.operator.arithmetic",
                "keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#77abc5"
            }
        },
        {
            "scope": [
                "punctuation.parenthesis",
                "punctuation.definition.arguments",
                "punctuation.separator",
                "punctuation.section.class.begin",
                "punctuation.section.function.begin"
            ],
            "settings": {
                "foreground": "#9ea5b3"
            }
        },
        {
            "scope": [
                "punctuation.definition.decorator"
            ],
            "settings": {
                "foreground": "#9ba9d4"
            }
        },
        {
            "scope": [
                "keyword.other.DML.sql",
                "keyword.other.DDL",
                "keyword.other.alias.sql",
                "keyword.other.sql"
            ],
            "settings": {
                "foreground": "#77abc5"
            }
        },
        {
            "scope": [
                "constant.other.table-name.sql"
            ],
            "settings": {
                "foreground": "#84587c"
            }
        },
        {
            "scope": [
                "storage.type.string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#91ae8f"
            }
        },
        {
            "scope": [
                "constant.character.format.placeholder"
            ],
            "settings": {
                "foreground": "#6a8e67"
            }
        },
        {
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#456c42",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#778bc5"
            }
        },
        {
            "scope": [
                "support.variable.magic",
                "constant.other.caps"
            ],
            "settings": {
                "foreground": "#82353c"
            }
        },
        {
            "scope": [
                "support.type.property-name.json",
                "entity.name.tag.yaml"
            ],
            "settings": {
                "foreground": "#293d7a"
            }
        }
    ]
