    "semanticTokenColors": {
        "class": "#387aa8",
        "enum": "#387aa8",
        "function": "#387aa8",
        "function.decorator": "#387aa8",
        "method": "#387aa8",
        "class.declaration": {
            "foreground": "#387aa8",
            "fontStyle": "bold"
        },
        "function.declaration": {
            "foreground": "#387aa8",
            "fontStyle": "bold"
        },
        "method.declaration": {
            "foreground": "#387aa8",
            "fontStyle": "bold"
        },
        "module": {
            "foreground": "#387aa8",
            "fontStyle": "underline"
        },
        "selfParameter": {
            "foreground": "#9f6f97",
            "fontStyle": "bold"
        },
        "clsParameter": {
            "foreground": "#9f6f97",
            "fontStyle": "bold"
        },
        "property": "#84587c",
        "property.declaration": "#84587c",
        "enumMember": "#84587c",
        "class.typeHint": "#387aa8",
        "enum.typeHint": "#387aa8"
    },
    "tokenColors": [
        {
            "scope": [
                "meta.function-call.generic.python",
                "support.function.string.sql"
            ],
            "settings": {
                "foreground": "#387aa8"
            }
        },
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#c0c5ce",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "storage.type.class",
                "storage.type.function",
                "storage.modifier",
                "keyword.control.flow",
                "keyword.control.import",
                "keyword.operator.logical"
            ],
            "settings": {
                "foreground": "#5eb8c9",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "variable.parameter.function-call"
            ],
            "settings": {
                "foreground": "#5eb8c9"
            }
        },
        {
            "scope": [
                "meta.function-call.arguments keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#c0c5ce"
            }
        },
        {
            "scope": [
                "constant.language",
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#54924f"
            }
        },
        {
            "scope": [
                "keyword.operator.arithmetic",
                "keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#5eb8c9"
            }
        },
        {
            "scope": [
                "punctuation.parenthesis",
                "punctuation.definition.arguments",
                "punctuation.separator",
                "punctuation.section.class.begin",
                "punctuation.section.function.begin"
            ],
            "settings": {
                "foreground": "#c0c5ce"
            }
        },
        {
            "scope": [
                "punctuation.definition.decorator"
            ],
            "settings": {
                "foreground": "#94bedb"
            }
        },
        {
            "scope": [
                "keyword.other.DML.sql",
                "keyword.other.DDL",
                "keyword.other.alias.sql",
                "keyword.other.sql"
            ],
            "settings": {
                "foreground": "#5eb8c9"
            }
        },
        {
            "scope": [
                "constant.other.table-name.sql"
            ],
            "settings": {
                "foreground": "#84587c"
            }
        },
        {
            "scope": [
                "storage.type.string",
                "punctuation.definition.string,"
            ],
            "settings": {
                "foreground": "#79b474",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "constant.character.format.placeholder"
            ],
            "settings": {
                "foreground": "#54924f"
            }
        },
        {
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#396336",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "support.variable.magic",
                "constant.other.caps"
            ],
            "settings": {
                "foreground": "#82353c"
            }
        },
        {
            "scope": [
                "support.type.property-name.json",
                "entity.name.tag.yaml"
            ],
            "settings": {
                "foreground": "#265373"
            }
        }
    ]
